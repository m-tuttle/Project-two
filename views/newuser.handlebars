<div class="row">
    <div class="col s12 m6">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title">New User</span>

            </div>
            <div class="card-action">
                <input placeholder="ENTER ID" id="user_name" type="text" class="validate">
                <label for="username">User Name</label>
                <br>
                <input placeholder="ENTER PASSWORD" id="user_pw" type="password" class="validate">
                <label for="password">Password</label>
                <br>
                <input placeholder="ENTER ID" id="email" type="email" class="validate">
                <label for="username">E-mail address</label>
                <br>
                <input placeholder="ENTER PASSWORD" id="phone" type="text" class="validate">
                <label for="password">Phone</label>
                <br>
                <a class="waves-effect waves-light btn" id="signUpButton">Create Account!</a>
                <br>
                <br>

            </div>
        </div>
    </div>
</div>